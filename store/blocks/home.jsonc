{
  "store.home": { 
    "blocks": [
      "composition-component"
    ]
  },

  "composition-component": {
    "props": {
      "title": "Ejemplo de composición"
    },
    "blocks": ["solid-border", "dotted-border"],
    "children": ["list-context.product-list#composition", "image-new#dummy"]
  },

  "rich-text#title": { 
    "props": { 
      "text": "## Sección de productos",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "font": "t-heading-1"
    }
  },

  "solid-border": {
    "props": {
      "color": "blue"
    },
    "children": ["rich-text#title"]
  },

  "dotted-border": {
    "props": {
      "color": "green"
    },
    "children": ["rich-text#title"]
  },


  "image-new#dummy": {
    "props": {
      "src": "https://vtexhelp.vtexassets.com/_v/public/assets/v1/published/vtexhelp.help-io@2.7.0/public/logo.png"
    }
  }

}